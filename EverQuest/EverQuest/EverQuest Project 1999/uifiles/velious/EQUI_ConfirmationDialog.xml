  <!-- Jibartik: Thisis the start of a static background animation -->
  <!--Jib: Background texture -->
  <!--Jib: Background texture load END -->
  <Ui2DAnimation item="Jib_ConfBGTexture">
    <Cycle>true</Cycle>
    <Frames>
      <Texture>classic_backgrounds03.png</Texture>
      <Location>
        <X>120</X>
        <!-- changes texture postion -->
        <Y>0</Y>
        <!-- changes texture postion -->
      </Location>
      <Size>
        <CX>197</CX>
        <!-- changes size of image -->
        <CY>117</CY>
        <!-- changes size of image -->
      </Size>
      <Duration>1000</Duration>
    </Frames>
  </Ui2DAnimation>
  <!-- this is the end of the pressed button -->
  <!-- Jibartik Start: This positions a static aniamtion on the ui -->
  <StaticAnimation item="Jib_ConfBackgroud">
    <RelativePosition>true</RelativePosition>
    <Location>
      <X>0</X>
      <!-- will move the image around -->
      <Y>0</Y>
      <!-- will move the image around -->
    </Location>
    <Size>
      <CX>197</CX>
      <!-- changes size of image -->
      <CY>117</CY>
      <!-- changes size of image -->
    </Size>
    <Animation>Jib_ConfBGTexture</Animation>
  </StaticAnimation>
  <!-- END: place a static anaitmion -->
