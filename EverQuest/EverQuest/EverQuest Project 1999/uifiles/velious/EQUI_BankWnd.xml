  <Ui2DAnimation item="Jib_BankBackgroundTexture">
    <Cycle>true</Cycle>
    <Frames>
      <Texture>classic_backgrounds03.png</Texture>
      <Location>
        <X>0</X>
        <!-- changes texture postion -->
        <Y>0</Y>
        <!-- changes texture postion -->
      </Location>
      <Size>
        <CX>119</CX>
        <!-- changes size of image -->
        <CY>505</CY>
        <!-- changes size of image -->
      </Size>
      <Duration>1000</Duration>
    </Frames>
  </Ui2DAnimation>
